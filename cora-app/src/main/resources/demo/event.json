{
  "event": {
    "eventName": "TempSensor/${id}/update_temp",
    "data": {
      "temp": "${temp}"
    }
  },
  "event": {
    "eventName": "AirPurifier/${id}/speed_up",
    "duration": "1000",
    "data": {
